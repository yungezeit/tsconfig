{
  "eslint.validate": ["javascript", "javascriptreact", "yaml", "json", "jsonc", "json5"],
  "editor.defaultFormatter": "biomejs.biome",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "quickfix.biome": "explicit"
  },
  "files.associations": {
    "default.json": "jsonc",
    "dom.json": "jsonc",
  }
}
